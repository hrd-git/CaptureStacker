<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
    </startup>
    <appSettings>
      <add key="HotKey" value="LShiftKey"/>
      <add key="ImageStoragePath" value="ImageStorage/"/>
      <add key="RootDirectoryPath" value="../CaptureStacker"/>
      <add key="FrameBorderSize" value="5"/>
      <add key="CaptureStackerWidth" value="280"/>
      <add key="CaptureStackerHeight" value="85"/>
      <add key="PreviewWidth" value="580"/>
      <add key="PreviewHeight" value="600"/>
      <add key="FileName" value="Image"/>
      <add key="Extension" value=".jpg"/>
    </appSettings>
</configuration>
